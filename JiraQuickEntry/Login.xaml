<Window x:Class="JiraQuickEntry.Login"
		xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		Title="Login"
		Height="300"
		Width="300"
		ResizeMode="NoResize">
	<Grid>
		<TextBox Height="23"
				 HorizontalAlignment="Left"
				 Margin="79,62,0,0"
				 Name="Username"
				 VerticalAlignment="Top"
				 Width="120" />
		<TextBox Height="23"
				 HorizontalAlignment="Left"
				 Margin="79,127,0,0"
				 Name="Password"
				 VerticalAlignment="Top"
				 Width="120" />
		<Button Content="Login"
				Height="23" IsDefault="True"
				HorizontalAlignment="Center"
				Margin="100,0,104,58"
				Name="button1"
				VerticalAlignment="Bottom"
				Width="75"
				Click="button1_Click" />
		<TextBlock Height="23"
				   HorizontalAlignment="Left"
				   Margin="79,43,0,0"
				   Name="textBlock1"
				   Text="Username"
				   VerticalAlignment="Top" />
		<TextBlock Height="23"
				   HorizontalAlignment="Left"
				   Margin="79,0,0,140"
				   Name="textBlock2"
				   Text="Password"
				   VerticalAlignment="Bottom" />
	</Grid>
</Window>
